{
  "Logging": {
    "IncludeScopes": false,
    "LogLevel": {
      "Default": "Warning"
    }
  },
  "Api": {
    "UpsertOnPut": false
  },
  "Jobs": {
    "DelayMs": 10000
  },
  "Simulate": {
    "Delay": {
      "Enabled": false,
      "Methods": [ "GET", "POST", "PUT", "PATCH", "DELETE" ],
      "MinMs": 500,
      "MaxMs": 5000
    },
    "Error": {
      "Enabled": false,
      "Methods": [ "POST", "PUT", "PATCH", "DELETE" ],
      "Probability": 50
    }
  }
}